              "});",
              "pm.test(\"email should be equal to 'Eliseo@gardner.biz'\", () => {",
              "    pm.expect(response[0].email).to.equal(\"Eliseo@gardner.biz\");",
              "});",
              "pm.test(\"id should be equal to 3\", () => {",
              "    pm.expect(response[2].id).to.equal(3);",
              "});",
              "pm.test(\"name should be equal to 'quo vero reiciendis velit similique earum'\", () => {",
              "    pm.expect(response[1].name).to.equal(\"quo vero reiciendis velit similique earum\");",
              "});"
            ],
